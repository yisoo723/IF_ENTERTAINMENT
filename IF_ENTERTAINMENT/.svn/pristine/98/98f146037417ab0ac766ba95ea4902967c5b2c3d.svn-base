/* 성이수 */
.top_area_box ul li a {
	padding: 0;
}
/* 성이수 */

.top_area_box ul {
	margin-bottom: -1px;
}

.top_menu {
	margin-right: 0;
	margin-top: 5px;
}

.nav_box {
	background: #773CD5 none repeat scroll 0 0;
	border-radius: 0;
	position: fixed;
	top: 90px;
	z-index: 10;
}

.nav_box li:hover {
	background: #773CD5;
	font-weight: bold;
}

.nav_box li {
	padding: 20px 35px;
}

.animation-blink {
    animation: blink 1s infinite; /* 깜빡거리는 애니메이션을 적용합니다. */
}

@keyframes blink {
    0% {
        opacity: 1;
    }
    50% {
        opacity: 0;
    }
    100% {
        opacity: 1;
    }
}
#Signin{
