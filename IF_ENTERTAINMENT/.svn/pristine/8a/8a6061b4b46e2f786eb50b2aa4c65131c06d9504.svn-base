package kr.or.ddit.vo.community;


import lombok.Data;

@Data
public class CalVO {
	private String _id;
    private String title;
    private String start;
    private String end;
    private String description;
    private String type;
    private String username;
    private String backgroundColor;
    private String textColor;
    private boolean allDay;
    private String agId;
    
    CalVO(){}
    
    public CalVO(String _id, String title, String start, String end, String dest, String type, String username, String back, String color,String agId, boolean all){
    	this._id = _id;
    	this.title = title;
    	this.start = start;
    	this.end = end;
    	this.description = dest;
    	this.type = type;
    	this.username = username;
    	this.backgroundColor = back;
    	this.textColor = color;
    	this.agId = agId;
    	this.allDay = all;
    }
}
